[binaries]
c = '/usr/bin/i686-pc-cygwin-gcc'
cpp = '/usr/bin/i686-pc-cygwin-g++'
ar = '/usr/i686-pc-cygwin/bin/ar'
strip = '/usr/i686-pc-cygwin/bin/strip'
windres = '/usr/bin/i686-pc-cygwin-windres'
pkgconfig = '/usr/bin/i686-pc-cygwin-pkg-config'

[host_machine]
system = 'cygwin'
cpu_family = 'x86'
cpu = 'i686'
endian = 'little'
